{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "routes": [{ "src": "(.*)", "dest": "index.js" 
      },
      {
        "src": "/favicon.ico",
        "headers": { "Cache-Control": "public, max-age=3600" },
        "continue": true
      },
      {
        "src": "/uploadImage/(.*)",
        "headers": { "Cache-Control": "public, max-age=3600" },
        "continue": true,
        "dest" : "/uploadImage/(.*)"
        
      }
    ]
  }